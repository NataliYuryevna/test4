                  "& when (@reverseContent)": {
                    "&.mbr-col-lg-7,&.mbr-col-lg-8": {
                      "padding-right": "70px",
                      "& when (@fullWidth)": {
                        "@media (min-width: 1200px)": {
                          "padding-left": "15%"
                        }
                      }
                    },
                    "&.mbr-col-lg-6": {
                      "padding-right": "90px",
                      "& when (@fullWidth)": {
                        "@media (min-width: 1200px)": {
