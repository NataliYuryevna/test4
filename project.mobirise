      "components": [
        {
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 15px)",
              "padding-bottom": "(@paddingBottom * 15px)"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".twoBg": {
              "position": "absolute",
              "background-color": "@twoColor",
              "top": "0",
              "left": "0",
              "width": "100%",
              "height": "100%",
              "padding": "0",
              "z-index": "0"
            },
            ".mbr-pb-3": {
              "padding-bottom": "10px"
            },
            ".mbr-pt-5": {
              "padding-top": "30px"
            },
            ".mbr-pb-5": {
              "padding-bottom": "50px"
            },
            ".mbr-pt-4": {
              "padding-top": "20px"
            },
            ".slider-box, amp-carousel": {
              "position": "relative",
              "height": "(@slideHeight * 1px)",
              "width": "100%"
            },
            ".image-wrap": {
              "amp-img,img": {
                "height": "~\"@{heightImg}rem\""
              },
              "padding": "0"
            },
            ".mbr-row": {
              ".card-wrap, .card-box": {
                "position": "relative",
                "width": "100%",
                "z-index": "1"
              },
              "align-items": "center"
            },
            "& when not (@transparentBg)": {
              ".card-wrap .mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              }
            },
            "& when (@stylizedCounters = \"1\")": {
              ".slider-box": {
                "counter-reset": "decimal"
              },
              ".card-marker:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "& when (@countersBorder)": {
                  "border-bottom": "3px solid @primaryColor"
                }
              }
            },
            "& when (@stylizedCounters = \"2\")": {
              ".slide": {
                "list-style-type": "none"
              },
              ".card-marker:before": {
                "content": "\"\"",
                "border": "3px solid @primaryColor",
                "width": "~\"calc(@{widthImg}rem - 6px)\"",
                "height": "~\"calc(@{widthImg}rem - 6px)\"",
                "box-sizing": "initial",
                "display": "block"
              }
            },
            ".slide .mbr-flex": {
              "flex-direction": "row",
              "align-items": "baseline",
              "& when (@cardAlign = \"2\")": {
                "justify-content": "center"
              },
              "& when (@cardAlign = \"3\")": {
                "justify-content": "flex-end"
              },
              ".card-marker, .card-icon": {
                "margin-right": "15px"
              }
            },
            ".card-icon .amp-iconfont": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\"",
              "height": "auto"
            },
            "amp-carousel .slide, .slide .card-row": {
              "align-self": "flex-start"
            },
            ".image-wrap amp-img img": {
              "object-fit": "cover"
            },
            "& when (@showPadding)": {
              "@media (min-width: 992px)": {
