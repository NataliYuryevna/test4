            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".twoBg": {
              "position": "absolute",
              "background-color": "@twoColor",
              "top": "0",
              "left": "0",
              "width": "100%",
              "height": "100%",
              "padding": "0",
              "z-index": "0"
            },
            ".mbr-row": {
              ".card-wrap, .card-box": {
                "position": "relative",
                "width": "100%",
                "z-index": "1"
              }
            },
            "& when not (@transparentBg)": {
              ".card-wrap .mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              }
            },
            ".image-wrap": {
              "padding": "0"
            },
            ".mbr-pb-3": {
              "padding-bottom": "10px"
            },
            ".mbr-pb-5": {
              "padding-bottom": "30px"
            },
            ".mbr-pt-5": {
              "padding-top": "30px"
            },
            ".mbr-pt-4": {
              "padding-top": "20px"
            },
            "& when (@stylizedCounters = \"1\")": {
              ".mbr-list": {
                "counter-reset": "decimal"
              },
              ".card-marker:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "& when (@countersBorder)": {
                  "border-bottom": "3px solid @primaryColor"
                }
              }
            },
            "& when (@stylizedCounters = \"2\")": {
              ".mbr-list": {
                "list-style-type": "none"
              },
              ".card-marker:before": {
                "content": "\"\"",
                "border": "3px solid @primaryColor",
                "width": "~\"@{widthImg}rem\"",
                "height": "~\"@{widthImg}rem\"",
                "display": "block"
              }
            },
            ".mbr-list .mbr-flex": {
              "flex-direction": "row",
              "align-items": "baseline",
              "& when (@cardAlign = \"2\")": {
                "justify-content": "center"
              },
              "& when (@cardAlign = \"3\")": {
                "justify-content": "flex-end"
              },
              ".card-marker, .card-icon": {
                "margin-right": "15px"
              }
            },
            ".card-icon .amp-iconfont": {
              "color": "@primaryColor",
              "width": "~\"@{widthImg}rem\"",
              "height": "auto"
            },
            ".image-wrap amp-img img": {
              "object-fit": "cover"
            },
            "& when (@showPadding)": {
